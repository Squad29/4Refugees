@model IEnumerable<ForRefugees.Models.Avaliacao>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Positivo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Negativo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dataAvaliacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contratante)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Refugiado)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Positivo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Negativo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dataAvaliacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contratante.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Refugiado.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
